{
    "name" : "telkomselcontentaggregator/cli-task",
    "description": "Aggregator task queue image processing",
    "version": "1.0.0",
    "authors": [
        {
            "name": "ArrayIterator",
            "homepage": "https://github.com/arrayiterator",
            "role": "programmer"
        }
    ],
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-pdo_sqlite": "*",
        "symfony/console": "^6.2",
        "symfony/yaml": "^6.2",
        "php-ffmpeg/php-ffmpeg": "^1.0",
        "aws/aws-sdk-php": "^3.252",
        "google/apiclient": "^2.12"
    },
    "extra": {
        "google/apiclient-services": [
            "Drive"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7.1",
        "jetbrains/phpstorm-stubs": "^v2022.2",
        "humbug/box" : "^4.2.0",
        "phpunit/phpunit": "^9.5.26",
        "nikic/php-parser": "^v4.15.2"
    },
    "autoload": {
        "psr-4": {
            "TelkomselAggregatorTask\\": "app/"
        },
        "psr-0": {
            "TelkomselAggregatorTask\\": "app/"
        }
    }
}
